{
  "name": "__MSG_appName__",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon.png",
      "38": "images/icon.png"
    }
  },
  "permissions": [
    "https://github.com/*/*/issues/*",
    "http://github.com/*/*/issues/*",
    "background"
  ],
  "content_scripts": [{
    "matches": [
      "https://github.com/*/*/issues/*",
      "http://github.com/*/*/issues/*",
      "http://127.0.0.1:5000/*"
    ],
    "js": [
      "scripts/contentscript.js"
    ],
    "css": [
      "styles/content.css"
    ],
    "run_at": "document_end",
    "all_frames": false
  }],
  "web_accessible_resources": [
    "images/icon-48.png"
  ],
  "homepage_url": "https://github.com/wooodhead/github-issue-toc"
}
